<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="mainBorderPane" stylesheets="css/styles.css" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.thirstygoat.kiqo.gui.MainController">
  <center>
    <SplitPane fx:id="mainSplitPane" dividerPositions="0.31" maxHeight="-1.0" minHeight="-1.0" BorderPane.alignment="CENTER">
      <items>
        <VBox fx:id="listPane" opacity="1.0" minWidth="270" maxWidth="270" spacing="10.0">
          <children>
            <fx:include fx:id="sideBar" source="sidebar.fxml" />
          </children>
        </VBox>
        <fx:include fx:id="detailsPane" source="detailsPane/main.fxml" />
      </items>
    </SplitPane>
  </center>
</BorderPane>
